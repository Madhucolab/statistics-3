    "# 25.Write a Python script to perform a Z-test for comparing proportions between two datasets or groups.\n",
    "import numpy as np\n",
    "import scipy.stats as stats\n",
    "\n",
    "# Function to perform Z-test for comparing two proportions\n",
    "def z_test_proportions(x1, n1, x2, n2, alpha=0.05):\n",
    "    # Sample proportions\n",
    "    p1_hat = x1 / n1\n",
    "    p2_hat = x2 / n2\n",
    "    \n",
    "    # Pooled sample proportion\n",
    "    P = (x1 + x2) / (n1 + n2)\n",
    "    \n",
    "    # Z-test statistic\n",
    "    Z = (p1_hat - p2_hat) / np.sqrt(P * (1 - P) * (1/n1 + 1/n2))\n",
    "    \n",
    "    # Critical value for a two-tailed test (95% confidence)\n",
    "    Z_critical = stats.norm.ppf(1 - alpha / 2)\n",
    "    \n",
    "    # Calculate the p-value for the test statistic\n",
    "    p_value = 2 * (1 - stats.norm.cdf(abs(Z)))\n",
    "    \n",
    "    # Results\n",
    "    print(f\"Z-test Statistic: {Z:.4f}\")\n",
    "    print(f\"Critical Value (Z): {Z_critical:.4f}\")\n",
    "    print(f\"P-value: {p_value:.4f}\")\n",
    "    \n",
    "    if abs(Z) > Z_critical:\n",
    "        print(\"Reject the null hypothesis: The proportions are significantly different.\")\n",
    "    else:\n",
    "        print(\"Fail to reject the null hypothesis: The proportions are not significantly different.\")\n",
    "\n",
    "# Example data\n",
    "x1 = 150  # Number of successes in group 1\n",
    "n1 = 500  # Sample size of group 1\n",
    "\n",
    "x2 = 120  # Number of successes in group 2\n",
    "n2 = 450  # Sample size of group 2\n",
    "\n",
    "# Perform Z-test\n",
    "z_test_proportions(x1, n1, x2, n2)\n"
