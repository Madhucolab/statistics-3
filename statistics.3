    "\n",
    "# Function to simulate hypothesis testing and calculate Type I and Type II errors\n",
    "def visualize_type_errors(population_mean, sample_size, alpha, effect_size, num_simulations=10000):\n",
    "    \n",
    "    \n",
    "    # Type I Error (False Positive) - Null hypothesis is true\n",
    "    type_1_errors = 0\n",
    "    \n",
    "    # Type II Error (False Negative) - Null hypothesis is false (alternative hypothesis is true)\n",
    "    type_2_errors = 0\n",
    "    \n",
    "    # Perform simulations\n",
    "    for _ in range(num_simulations):\n",
    "        # Simulate sample under null hypothesis (population mean = population_mean)\n",
    "        sample_null = np.random.normal(population_mean, 1, sample_size)\n",
    "        sample_alt = np.random.normal(population_mean + effect_size, 1, sample_size)\n",
    "        \n",
    "        # Perform a one-sample t-test for the null hypothesis\n",
    "        t_stat_null, p_value_null = stats.ttest_1samp(sample_null, population_mean)\n",
    "        t_stat_alt, p_value_alt = stats.ttest_1samp(sample_alt, population_mean)\n",
    "        \n",
    "        # Type I Error: Reject null hypothesis when it is true (p-value < alpha)\n",
    "        if p_value_null < alpha:\n",
    "            type_1_errors += 1\n",
    "        \n",
    "        # Type II Error: Fail to reject null hypothesis when it is false (p-value >= alpha)\n",
    "        if p_value_alt >= alpha:\n",
    "            type_2_errors += 1\n",
    "    \n",
    "    # Calculate Type I and Type II error rates\n",
    "    type_1_error_rate = type_1_errors / num_simulations\n",
