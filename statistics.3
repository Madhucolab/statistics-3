    "# 26.Implement an F-test for comparing the variances of two datasets, then interpret and visualize the results.\n",
    "import numpy as np\n",
    "import scipy.stats as stats\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Function to perform the F-test for comparing variances\n",
    "def f_test_variances(data1, data2, alpha=0.05):\n",
    "    # Sample variances\n",
    "    var1 = np.var(data1, ddof=1)  # sample variance of data1\n",
    "    var2 = np.var(data2, ddof=1)  # sample variance of data2\n",
    "    \n",
    "    # Sample sizes\n",
    "    n1 = len(data1)\n",
    "    n2 = len(data2)\n",
    "    \n",
    "    # F-test statistic (larger variance / smaller variance)\n",
    "    F_statistic = var1 / var2 if var1 > var2 else var2 / var1\n",
    "    \n",
    "    # Degrees of freedom\n",
    "    df1 = n1 - 1\n",
    "    df2 = n2 - 1\n",
    "    \n",
    "    # Critical value from the F-distribution for a two-tailed test\n",
    "    F_critical_upper = stats.f.ppf(1 - alpha / 2, df1, df2)\n",
    "    F_critical_lower = stats.f.ppf(alpha / 2, df1, df2)\n",
    "    \n",
    "    # Calculate the p-value for the F-statistic\n",
    "    p_value = 2 * min(stats.f.cdf(F_statistic, df1, df2), 1 - stats.f.cdf(F_statistic, df1, df2))\n",
    "    \n",
    "    # Print the results\n",
    "    print(f\"Sample variance of data1: {var1:.4f}\")\n",
    "    print(f\"Sample variance of data2: {var2:.4f}\")\n",
    "    print(f\"F-statistic: {F_statistic:.4f}\")\n",
    "    print(f\"Critical values: Lower = {F_critical_lower:.4f}, Upper = {F_critical_upper:.4f}\")\n",
    "    print(f\"P-value: {p_value:.4f}\")\n",
    "    \n",
    "    # Interpretation of results\n",
    "    if F_statistic < F_critical_lower or F_statistic > F_critical_upper:\n",
    "        print(\"Reject the null hypothesis: The variances are significantly different.\")\n",
    "    else:\n",
    "        print(\"Fail to reject the null hypothesis: The variances are not significantly different.\")\n",
    "\n",
    "# Simulating two datasets\n",
    "np.random.seed(42)\n",
    "\n",
    "# Dataset 1 (higher variance)\n",
    "data1 = np.random.normal(loc=0, scale=10, size=100)\n",
