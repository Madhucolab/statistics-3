    "# 24 Perform a hypothesis test for population variance using a Chi-square distribution and interpret the results.\n",
    "import numpy as np\n",
    "import scipy.stats as stats\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Step 1: Simulate data (random sample from normal distribution)\n",
    "np.random.seed(42)\n",
    "\n",
    "# Sample size and hypothesized population variance\n",
    "n = 30\n",
    "sigma_0_squared = 100  # Hypothesized population variance\n",
    "\n",
    "# Simulating a sample with true population variance 100\n",
    "sample = np.random.normal(loc=0, scale=np.sqrt(sigma_0_squared), size=n)\n",
    "\n",
    "# Step 2: Calculate sample variance\n",
    "sample_variance = np.var(sample, ddof=1)  # ddof=1 for sample variance (Bessel's correction)\n",
    "\n",
    "# Step 3: Calculate the chi-square test statistic\n",
    "chi_square_statistic = (n - 1) * sample_variance / sigma_0_squared\n",
    "\n",
    "# Step 4: Define the significance level and calculate critical values\n",
    "alpha = 0.05\n",
    "df = n - 1  # degrees of freedom\n",
    "\n",
    "# Calculate the critical values for a two-tailed test\n",
    "critical_value_lower = stats.chi2.ppf(alpha / 2, df)\n",
    "critical_value_upper = stats.chi2.ppf(1 - alpha / 2, df)\n",
    "\n",
    "# Step 5: Perform hypothesis test and interpret results\n",
    "print(f\"Sample variance: {sample_variance}\")\n",
    "print(f\"Chi-square statistic: {chi_square_statistic}\")\n",
    "print(f\"Critical values: Lower = {critical_value_lower}, Upper = {critical_value_upper}\")\n",
    "\n",
    "# Check if we reject the null hypothesis\n",
    "if chi_square_statistic < critical_value_lower or chi_square_statistic > critical_value_upper:\n",
    "    print(\"Reject the null hypothesis: The population variance is different from the hypothesized value.\")\n",
    "else:\n",
    "    print(\"Fail to reject the null hypothesis: The population variance is not significantly different from the hypothesized value.\")\n",
    "\n",
    "# Step 6: Visualize the chi-square distribution and the calculated statistic\n",
    "x = np.linspace(0, 50, 500)\n",
    "y = stats.chi2.pdf(x, df)\n",
    "\n",
    "plt.figure(figsize=(8, 6))\n",
    "plt.plot(x, y, label='Chi-square distribution', color='blue')\n",
    "plt.axvline(chi_square_statistic, color='red', linestyle='--', label=f'Chi-square statistic = {chi_square_statistic:.2f}')\n",
