   "source": [
    "# 27.Perform a Chi-square test for goodness of fit with simulated data and analyze the results.\n",
    "import numpy as np\n",
    "import scipy.stats as stats\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Function to perform Chi-square goodness of fit test\n",
    "def chi_square_goodness_of_fit(observed, expected, alpha=0.05):\n",
    "    # Calculate the chi-square statistic and p-value\n",
    "    chi2_stat, p_value = stats.chisquare(observed, expected)\n",
    "    \n",
    "    # Degrees of freedom\n",
    "    df = len(observed) - 1\n",
    "    \n",
    "    # Critical value from chi-square distribution for a given alpha and df\n",
    "    chi2_critical = stats.chi2.ppf(1 - alpha, df)\n",
    "    \n",
    "    # Print the results\n",
    "    print(f\"Chi-square Statistic: {chi2_stat:.4f}\")\n",
    "    print(f\"P-value: {p_value:.4f}\")\n",
    "    print(f\"Degrees of Freedom: {df}\")\n",
    "    print(f\"Critical Value: {chi2_critical:.4f}\")\n",
    "    \n",
    "    # Decision\n",
    "    if chi2_stat > chi2_critical:\n",
    "        print(\"Reject the null hypothesis: The observed frequencies do not match the expected distribution.\")\n",
    "    else:\n",
    "        print(\"Fail to reject the null hypothesis: The observed frequencies match the expected distribution.\")\n",
    "\n",
    "# Simulate a dice roll experiment\n",
    "np.random.seed(42)\n",
    "\n",
    "# Simulate 100 rolls of a fair 6-sided die\n",
    "n_rolls = 100\n",
    "observed_rolls = np.random.randint(1, 7, size=n_rolls)\n",
    "\n",
    "# Count the frequency of each face (1 through 6)\n",
    "observed_counts = [np.sum(observed_rolls == i) for i in range(1, 7)]\n",
    "\n",
    "# Expected frequencies (for a fair die, each face should appear with probability 1/6)\n",
    "expected_counts = [n_rolls / 6] * 6\n",
    "\n",
    "# Perform the Chi-square goodness of fit test\n",
    "chi_square_goodness_of_fit(observed_counts, expected_counts)\n",
    "\n",
    "# Visualization of the observed vs expected frequencies\n",
    "labels = [str(i) for i in range(1, 7)]\n",
    "x = np.arange(len(labels))\n",
    "\n",
    "fig, ax = plt.subplots(figsize=(8, 6))\n",
    "\n",
    "# Bar plot for observed and expected frequencies\n",
    "ax.bar(x - 0.2, observed_counts, 0.4, label='Observed', color='blue')\n",
    "ax.bar(x + 0.2, expected_counts, 0.4, label='Expected', color='orange')\n",
    "\n",
    "ax.set_xlabel('Die Face')\n",
    "ax.set_ylabel('Frequency')\n",
    "ax.set_title('Chi-Square Goodness of Fit Test: Observed vs Expected Frequencies')\n",
    "ax.set_xticks(x)\n",
    "ax.set_xticklabels(labels)\n",
    "ax.legend()\n",
    "\n",
    "plt.show()\n"
   ]
