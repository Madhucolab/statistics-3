    "# 10.Write a Python program to calculate the margin of error for a given confidence level using sample data.\n",
    "import numpy as np\n",
    "from scipy import stats\n",
    "\n",
    "def calculate_margin_of_error(sample_data, confidence_level=0.95):\n",
    "  \n",
    "    # Sample size\n",
    "    n = len(sample_data)\n",
    "    \n",
    "    # Sample standard deviation (using ddof=1 for sample std deviation)\n",
    "    sample_std = np.std(sample_data, ddof=1)\n",
    "    \n",
    "    # Sample mean (not directly used for margin of error calculation but useful for reference)\n",
    "    sample_mean = np.mean(sample_data)\n",
    "    \n",
    "    # T-distribution critical value (for small sample sizes, assuming unknown population std)\n",
    "    t_critical = stats.t.ppf((1 + confidence_level) / 2, df=n-1)\n",
    "    \n",
    "    # Standard error of the mean (SEM)\n",
    "    sem = sample_std / np.sqrt(n)\n",
    "    \n",
    "    # Margin of error\n",
